          <div ng-show="true"> <!-- add farmer notifications -->
            <span class="admin-page-title">Approval Requests :</span><span class="admin-page-title-cat"> Farmer</span>
            <div class="thumbnail" ng-repeat = "farmer in pendingFarmers">
              <div class="row row-margin-none">
                <div class="col-md-10">
                  <div class="row">
                    <div class="col-md-2 approve-col-1">
                      <h5>Name <span class="pull-right">:</span></h5>
                    </div>
                    <div class="col-md-6">
                      <span>{{farmer.FIRST_NAME}} {{farmer.LAST_NAME}}</span>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-md-2 approve-col-1">
                      <h5>Email Id <span class="pull-right">:</span></h5>
                    </div>
                    <div class="col-md-6">
                      <span>{{farmer.EMAIL}}</span>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-md-2 approve-col-1">
                      <h5>Address <span class="pull-right">:</span></h5>
                    </div>
                    <div class="col-md-6 address-line">
                      <p>{{farmer.ADDRESS}}</p>
                      <p>Apt 332</p>
                      <p>{{faremr.CITY}}, {{farmer.STATE}} {{farmer.ZIP}}</p>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-md-2 approve-col-1">
                      <h5>Phone <span class="pull-right">:</span></h5>
                    </div>
                    <div class="col-md-6">
                      <span>{{farmer.PHONE_NUMBER}}</span>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-md-2 approve-col-1">
                      <h5>SSN <span class="pull-right">:</span></h5>
                    </div>
                    <div class="col-md-6  ">
                      <span>XX-XX-XXX</span>
                    </div>
                  </div>
                </div>
                <div class="col-md-2">
                  <div class="row" ng-show="{{farmer.IS_APPROVED}} == 1">
                    <button class="btn btn-success btn-xs btn-theme btn-theme-narrow pull-right" ng-click="approveFarmer(farmer.USER_ID)">
                      <span class="pull-left">Approve</span>
                      <span class="badge pull-right">
                        <span class="glyphicon glyphicon-ok" aria-hidden="true">
                        </span>
                      </span>
                    </button>
                  </div>
                  <div class="row" ng-show="{{farmer.IS_APPROVED}} == 2">
                    <button class="btn btn-danger btn-xs btn-theme btn-theme-narrow pull-right" ng-click="rejectFarmer(farmer.USER_ID)">
                      <span class="pull-left">Reject</span>
                      <span class="badge pull-right">
                        <span class="glyphicon glyphicon-remove" aria-hidden="true">
                        </span>
                      </span>
                    </button>
                  </div>
                </div>
              </div>
            </div>
          </div><!-- FINISHED add farmer notification -->
