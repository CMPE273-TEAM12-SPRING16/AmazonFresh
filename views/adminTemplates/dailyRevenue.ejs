<div> <!-- Add product notofications -->
  <span class="admin-page-title">Daily Revenue :</span>
  <div class="thumbnail">
    <div class="row">
      <div class="col-md-3">
        <select class="form-control" ng-model="chartMonth">
          <option value="1">January</option>
          <option value="2">February</option>
          <option value="3">March</option>
          <option value="4">April</option>
          <option value="5">May</option>
          <option value="6">June</option>
          <option value="7">July</option>
          <option value="8">August</option>
          <option value="9">September</option>
          <option value="10">October</option>
          <option value="11">November</option>
          <option value="12">December</option>
        </select>
      </div>
      <div class="col-md-2"><button class="btn btn-primary btn-theme" ng-click="fetchChart()" style="margin: 0px !important;">Load Graph</button></div>
    </div>
    <div class="row" style="text-align : center;">
        <div fusioncharts
         width="90%"
         height="400"
         type="mscolumn2d"
         dataSource="{{myDataSource}}" >
      </div>
    </div>

  </div>
</div>
